        android:text="展示dataUrlBean1"/>
    <Button
        android:id="@+id/person1"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="展示person1"/>
    <Button
        android:id="@+id/response1"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="展示response1"/>
    <Button
        android:id="@+id/response2"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="展示response2"/>
    <Button
        android:id="@+id/showboy1"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="展示boy1"/>
            <Button
                android:id="@+id/int1"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:text="展示int"/>
            <Button
                android:id="@+id/long1"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:text="展示long"/>
            <Button
