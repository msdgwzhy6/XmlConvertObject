            <Button
                android:id="@+id/boolean1"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:text="展示boolean1"/>
            <Button
                android:id="@+id/boolean2"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:text="展示boolean2"/>
            <Button
                android:id="@+id/map1"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:text="展示map"/>
