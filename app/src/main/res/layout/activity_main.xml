            android:layout_width="match_parent"
            android:layout_height="wrap_content">

    <Button
        android:id="@+id/showlist1"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="展示list1"/>
    <Button
        android:id="@+id/showlist2"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="展示list2"/>
    <Button
        android:id="@+id/showlist3"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="展示list3"/>
